@model IEnumerable<finalaspphone.Models.Baja>

@{
    ViewBag.Title = "Lista baja";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<h2>Lista de telefonos para dar de baja</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.cedula)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.numero)
        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.cedula)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.numero)
            </td>
            <td>
                @Html.ActionLink("Deletear", "Deletear", new { cedula = item.cedula, telefono = item.numero })
            </td>
        </tr>
    }

</table>
